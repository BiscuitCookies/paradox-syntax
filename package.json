{
    "contributors": [
        "Gratak",
        "Dayshine"
    ],
    "name": "paradox-syntax",
    "displayName": "Paradox Syntax Highlighting",
    "description": "",
    "version": "0.1.8",
    "publisher": "tboby",
    "repository": "https://github.com/tboby/paradox-syntax",
    "preview": true,
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "hoi4"
            },
            {
                "id": "stellaris"
            },
            {
                "id": "ck2"
            },
            {
                "id": "eu4"
            },
            {
                "id": "imperator"
            }
        ],
        "grammars": [
            {
                "language": "stellaris",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.stellaris",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/stellaris.tmLanguage.json"
            },
            {
                "language": "hoi4",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.hoi4",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/hoi4.tmLanguage.json"
            },
            {
                "language": "eu4",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.eu4",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/eu4.tmLanguage.json"
            },
            {
                "language": "ck2",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.ck2",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/ck2.tmLanguage.json"
            },
            {
                "language": "imperator",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "Stellaris Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/Paradox-Dimmed.tmLanguage.json"
            },
            {
                "label": "Quiet Light Paradox Theme",
                "uiTheme": "vs",
                "path": "./themes/Paradox-QuietLight.tmLanguage.json"
            }
        ]
    }
}
